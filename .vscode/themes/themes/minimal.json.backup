  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
  {
      "scope": "comment",
      "settings": {
        "foreground": "#90909080"
      }
    },
    {
      "name": "Kontrol Alur & Deklarasi",
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ffab70"
      }
    },
    {
      "name": "Fungsi, Tipe & Struktur",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "entity.name.function",
        "meta.function-call",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#f5abae"
      }
    },
    {
      "name": "Nilai, Data & Variabel",
      "scope": [
        "string",
        "constant.numeric",
        "constant.language",
        "variable.language.this",
        "entity.other.attribute-name",
        "meta.property-name",
        "meta.property-value"
      ],
      "settings": {
        "foreground": "#90AED3"
      }
    }
